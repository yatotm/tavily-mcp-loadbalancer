# Service configuration
PORT=60002
HOST=0.0.0.0

# Database configuration
DATABASE_PATH=./data/tavily.db
DATABASE_ENCRYPTION_KEY=replace-with-32-byte-random-string
LOG_RETENTION_DAYS=30

# Load balancer configuration
MAX_CONCURRENT_REQUESTS=4
REQUEST_TIMEOUT=30000
MAX_KEY_ERRORS=5

# Admin UI configuration
ADMIN_PASSWORD=
ENABLE_WEB_UI=true

# Logging configuration
LOG_LEVEL=info
LOG_FORMAT=json

# Optional seed keys (imported on first start)
TAVILY_API_KEYS=tvly-dev-key1,tvly-dev-key2
# TAVILY_API_KEY=tvly-dev-single-key
